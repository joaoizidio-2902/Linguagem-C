#include <stdio.h>
#include <conio.h>

int main()
{
char d, u;
printf ("Voce ira informar 2 numeros a seguir, em dezena e unidade: \n");
printf ("\nInforme o numero em dezena: ");
d = getch();
printf ("\nInforme o numero em unidade: ");
u = getch();
printf("\nEntre com o valor da tabuada: %d",((d-48)*10 + (u-48)*1));

 printf ("\n%d * 1 = %d \n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*1);
 printf ("%d * 2 = %d\n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*2);
 printf ("%d * 3 = %d\n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*3);
 printf ("%d * 4 = %d\n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*4);
 printf ("%d * 5 = %d\n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*5);
 printf ("%d * 6 = %d\n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*6);
 printf ("%d * 7 = %d\n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*7);
 printf ("%d * 8 = %d\n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*8);
 printf ("%d * 9 = %d\n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*9);
 printf ("%d * 10 = %d\n", (d-48)*10 + (u-48)*1 , ((d-48)*10 + (u-48)*1)*10);
 
 return 0; 
 } 
